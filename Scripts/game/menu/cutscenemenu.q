SCRIPT SJ_spotlight_fade_out 
	<percent> = 100 
	BEGIN 
		<percent> = ( <percent> -4 ) 
		IF ( <percent> < 0 ) 
			<percent> = 0 
		ENDIF 
		Fakelights percent = <percent> id = TRG_levellight_01 time = 0 
		Wait 1 frames 
	REPEAT 25 
ENDSCRIPT

SCRIPT SJ_spotlight_fade_in 
	<percent> = 0 
	BEGIN 
		<percent> = ( <percent> + 4 ) 
		IF ( <percent> > 100 ) 
			<percent> = 100 
		ENDIF 
		Fakelights percent = <percent> id = TRG_levellight_01 time = 0 
		Wait 1 frames 
	REPEAT 25 
ENDSCRIPT

