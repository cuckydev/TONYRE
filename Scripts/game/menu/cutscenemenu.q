		<index> = ( <index> + 1 ) 
	REPEAT <array_size> 
	GetArraySize cutscene_unlock_list_hi 
	<index> = 0 
	BEGIN 
		SetGlobalFlag flag = ( ( ( cutscene_unlock_list_hi ) [ <index> ] ) . flag ) 
		<index> = ( <index> + 1 ) 
	REPEAT <array_size> 
	GetArraySize cutscene_unlock_list_vc 
	<index> = 0 
