SCRIPT unlock_all_cutscenes 
	GetArraySize cutscene_unlock_list_nj 
	<index> = 0 
	BEGIN 
		SetGlobalFlag flag = ( ( ( cutscene_unlock_list_nj ) [ <index> ] ) . flag ) 
		<index> = ( <index> + 1 ) 
	REPEAT <array_size> 
	GetArraySize cutscene_unlock_list_ny 
	<index> = 0 
	BEGIN 
		SetGlobalFlag flag = ( ( ( cutscene_unlock_list_ny ) [ <index> ] ) . flag ) 
		<index> = ( <index> + 1 ) 
